{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "UMD",
    "moduleResolution": "node",
    "outDir": "./build",
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {

    },
    "rootDir": ".",
    "inlineSourceMap": true,
    "lib": ["es5"],
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules"
  ]
}